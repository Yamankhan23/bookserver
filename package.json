{
  "name": "bookit-server",
  "version": "1.0.0",
  "description": "Backend API for BookIt travel booking platform",
  "type": "module",
  "main": "dist/server.js",
  "scripts": {
    "dev": "dotenv -e .env -- node --loader ts-node/esm src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "migrate": "dotenv -e .env -- prisma migrate dev --schema=./prisma/schema.prisma",
    "generate": "dotenv -e .env -- prisma generate --schema=./prisma/schema.prisma",
    "studio": "dotenv -e .env -- prisma studio",
    "seed": "dotenv -e .env -- ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/node": "^22.18.13",
    "dotenv-cli": "^11.0.0",
    "prisma": "^6.0.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.6.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "license": "MIT"
}
